{
  "Features": {
    "UseMockServices": true,
    "MockServiceLatency": 150,
    "ApiRootUrl": "https://dev-api.local:7234"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning"
    }
  },
  "Diagnostics": {
    "Serilog": {
      "UseAppDataDirectory": true,
      "FileName": "mobile-dev-.log",
      "EnableConsole": true,
      "MinimumLevel": "Debug"
    }
  }
}
